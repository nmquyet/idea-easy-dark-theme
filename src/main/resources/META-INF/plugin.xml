<idea-plugin>
  <id>com.qyengyn.easy-dark-theme</id>
  <name>Easy Dark Theme</name>
  <vendor email="minhquyet@gmail.com">qyengyn</vendor>

  <extensions defaultExtensionNs="com.intellij">
    <themeProvider id="com.qyengyn.easy-dark-theme" path="/easy_dark.theme.json"/>
    <bundledColorScheme id="com.qyengyn.easy-dark-color-scheme" path="/easy_dark.xml"/>
    <bundledColorScheme id="com.qyengyn.easy-dark-alternate-color-scheme" path="/easy_dark_alternate.xml"/>
    <annotator language="JAVA" implementationClass="com.qyengyn.easydarktheme.JavaExtendedKeywordAnnotator"/>
  </extensions>

  <depends>com.intellij.modules.platform</depends>
  <depends>com.intellij.java</depends>
</idea-plugin>
