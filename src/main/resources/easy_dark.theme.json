{
  "name": "Easy Dark",
  "dark": true,
  "author": "qyengyn",
  "editorScheme": "Easy Dark",
  "colors": {
    "background": "#2e2d2c",
    "darkerBackground": "#262525",
    "editor": "#32302F",
    "selection": "#094770",
    "text": "#BDAF93",
    "infoText": "#807766",
    "selectedText": "#EBDBB2",
    "inactiveSelection": "#383635",
    "accent": "#4B88C7",
    "border": "#403c3b"
  },
  "ui": {
    "*": {
      "foreground": "text",
      "background": "background",
      "selectionBackground": "selection",
      "selectionForeground": "selectedText",
      "separatorColor": "border",
      "infoForeground": "infoText",
      "borderColor": "border",
      "arc": 8
    },
    "Editor": {
      "background": "editor"
    },
    "Borders": {
      "color": "border",
      "ContrastBorderColor": "border"
    },
    "DefaultTabs": {
      "hoverBackground": "border"
    },
    "EditorTabs": {
      "unselectedBlend": "0.6",
      "unselectedAlpha": "0.6",
      "underlineHeight": 3,
      "hoverBackground": "border",
      "inactiveUnderlineColor": "accent",
      "underlineColor": "accent",
      "underlinedTabBackground": "background",
      "underlinedTabForeground": "selectedText"
    },
    "FileColor": {
      "Yellow": "#4d473a",
      "Green": "#363f2f",
      "Blue": "#2f344f",
      "Violet": "#362748",
      "Orange": "#4f3b31",
      "Rose": "#462643",
      "Gray": "#3d3d3d"
    },
    "Tree": {
      "modifiedItemForeground": "accent",
      "selectionInactiveBackground": "inactiveSelection",
      "Selection.arc": 3
    },
    "Notification.background": "darkerBackground",
    "Menu": {
      "background": "darkerBackground"
    },
    "PopupMenu": {
      "background": "darkerBackground"
    },
    "MenuItem": {
      "background": "darkerBackground",
      "acceleratorForeground": "infoText"
    },
    "Popup": {
      "background": "darkerBackground"
    },
    "PopupMenuSeparator": {
      "height": 13,
      "stripeIndent": 6,
      "stripeWidth": 1
    },
    "ToolTip" : {
      "background": "darkerBackground"
    },
    "Button": {
      "disabledText": "infoText"
    },
    "TextField": {
      "inactiveForeground": "infoText"
    },
    "ToolWindow": {
      "Header": {
        "inactiveBackground": "background"
      },
      "Button": {
        "hoverBackground": "inactiveSelection"
      }
    }
  }
}
