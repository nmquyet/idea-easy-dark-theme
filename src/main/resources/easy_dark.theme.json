{
  "name": "Easy Dark",
  "dark": true,
  "author": "qyengyn",
  "editorScheme": "Easy Dark",
  "colors": {
    "background": "#292828",
    "darkerBackground": "#262525",
    "editor": "#32302F",
    "selection": "#094770",
    "text": "#BDAF93",
    "textHighlight": "#CEBE9C",
    "infoText": "#807766",
    "selectedText": "#EBDBB2",
    "inactiveSelection": "#383635",
    "accent": "#3372ed",
    "borderLow": "#332f2c",
    "borderNeutral": "#3d3835",
    "border": "#4d4744",
    "notification": "#45494A"
  },
  "ui": {
    "*": {
      "foreground": "text",
      "background": "background",
      "selectionBackground": "selection",
      "selectionForeground": "selectedText",
      "separatorColor": "border",
      "infoForeground": "infoText",
      "borderColor": "border",
      "arc": 8
    },
    "Editor": {
      "Toolbar.borderColor": "borderNeutral",
      "background": "editor"
    },
    "Borders": {
      "color": "borderLow",
      "ContrastBorderColor": "border"
    },
    "TabbedPane": {
      "underlineColor": "accent"
    },
    "DefaultTabs": {
      "hoverBackground": "border",
      "underlineColor": "accent"
    },
    "EditorTabs": {
      "borderColor": "borderLow",
      "unselectedBlend": "0.6",
      "unselectedAlpha": "0.6",
      "underlineHeight": 3,
      "hoverBackground": "border",
      "inactiveUnderlineColor": "accent",
      "underlinedTabBackground": "background",
      "underlinedTabForeground": "selectedText"
    },
    "FileColor": {
      "Yellow": "#4d473a",
      "Green": "#363f2f",
      "Blue": "#2f344f",
      "Violet": "#362748",
      "Orange": "#4f3b31",
      "Rose": "#462643",
      "Gray": "#3d3d3d"
    },
    "Tree": {
      "foreground": "textHighlight",
      "modifiedItemForeground": "accent",
      "selectionBackground": "selectionBackground",
      "selectionInactiveBackground": "inactiveSelection",
      "Selection.arc": 3
    },
    "Menu": {
      "separatorColor": "borderNeutral",
      "background": "darkerBackground",
      "selectionBackground": "selection"
    },
    "PopupMenu": {
      "background": "darkerBackground"
    },
    "MenuItem": {
      "background": "darkerBackground",
      "acceleratorForeground": "infoText"
    },
    "Popup": {
      "background": "darkerBackground"
    },
    "PopupMenuSeparator": {
      "height": 13,
      "stripeIndent": 6,
      "stripeWidth": 1
    },
    "Component": {
      "borderColor": "borderNeutral"
    },
    "ToolTip" : {
      "background": "darkerBackground",
      "paintBorder": true,
      "borderColor": "#1c1c1c"
    },
    "Notification": {
      "background": "#45494A",
      "errorBackground": "#593d41",
      "borderColor": "#45494A",
      "ToolWindow": {
        "informativeBackground": "#45494A",
        "warningBackground": "#594321",
        "errorBackground": "#593d41"
      }

    },
    "Button": {
      "disabledText": "infoText"
    },
    "TextField": {
      "inactiveForeground": "infoText"
    },
    "ToolWindow": {
      "Header": {
        "borderColor": "borderNeutral",
        "inactiveBackground": "background"
      },
      "Button": {
        "hoverBackground": "inactiveSelection"
      }
    }
  }
}
