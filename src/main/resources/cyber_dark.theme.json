{
  "name": "Cyber Dark",
  "dark": true,
  "author": "qyengyn",
  "editorScheme": "/cyber_dark.xml",
  "colors": {
    "background": "#27282b",
    "darkerBackground": "#232426",
    "editor": "#32302F",
    "selection": "#094770",
    "text": "#b3afa2",
    "textHighlight": "#b3afa2",
    "infoText": "#666A73",
    "disabledText": "#44474d",
    "selectedText": "#cfc8ba",
    "inactiveSelection": "#383635",
    "accent": "#3372ed",
    "borderLow": "#2e3033",
    "borderNeutral": "#3a3c40",
    "border": "#47494d",
    "notification": "#45494A"
  },
  "icons": {
    "ColorPalette": {
      "Tree.iconColor": "#747A7F",
      "Gray.Stroke": "#bdbab5",
      "Gray.SecondaryStroke": "#bdbab5"
    }
  },
  "ui": {
    "*": {
      "foreground": "text",
      "selectionForeground": "selectedText",
      "background": "background",
      "selectionBackground": "selection",
      "infoForeground": "infoText",
      "separatorColor": "border",
      "borderColor": "border",
      "arc": 8
    },
    "TabbedPane": {
      "underlineColor": "accent"
    },
    "DefaultTabs": {
      "borderColor": "borderNeutral",
      "hoverBackground": "borderLow",
      "underlineColor": "accent"
    },
    "EditorTabs": {
      "background": "background",
      "borderColor": "borderNeutral",
      "unselectedBlend": "0.6",
      "unselectedAlpha": "0.6",
      "underlineHeight": 3,
      "underlinedTabBackground": "background",
      "underlinedTabForeground": "selectedText"
    },
    "Editor": {
      "Toolbar.borderColor": "borderNeutral",
      "background": "editor"
    },
    "Tree": {
      "foreground": "textHighlight",
      "modifiedItemForeground": "accent",
      "selectionInactiveForeground": "text",
      "selectionBackground": "selectionBackground",
      "selectionInactiveBackground": "inactiveSelection",
      "Selection.arc": 3
    },
    "Menu": {
      "separatorColor": "borderNeutral",
      "background": "darkerBackground",
      "selectionBackground": "selection"
    },
    "PopupMenu": {
      "background": "darkerBackground"
    },
    "MenuItem": {
      "background": "darkerBackground",
      "acceleratorForeground": "infoText",
      "disabledForeground": "disabledText"
    },
    "Popup": {
      "background": "darkerBackground"
    },
    "PopupMenuSeparator": {
      "height": 13,
      "stripeIndent": 6,
      "stripeWidth": 1
    },
    "Component": {
      "borderColor": "borderNeutral"
    },
    "ToolTip" : {
      "background": "darkerBackground",
      "paintBorder": true,
      "borderColor": "#1c1c1c"
    },
    "Notification": {
      "background": "#45494A",
      "errorBackground": "#593d41",
      "borderColor": "#45494A",
      "ToolWindow": {
        "informativeBackground": "#45494A",
        "warningBackground": "#594321",
        "errorBackground": "#593d41"
      }
    },
    "ToolWindow": {
      "Header": {
        "borderColor": "borderNeutral",
        "inactiveBackground": "background"
      },
      "Button": {
        "hoverBackground": "inactiveSelection"
      }
    },
    "Borders": {
      "color": "borderLow",
      "ContrastBorderColor": "border"
    },
    "FileColor": {
      "Yellow": "#332f2c",
      "Green": "#2c3326",
      "Blue": "#1e2133",
      "Violet": "#261c33",
      "Orange": "#332620",
      "Rose": "#331c31",
      "Gray": "#333333"
    },
    "Button": {
      "disabledText": "infoText"
    },
    "TextField": {
      "inactiveForeground": "infoText"
    }
  }
}
